{
  "name": "rag-observability-power",
  "version": "1.0.0",
  "description": "Kiro POWER for RAG system observability and self-improvement",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "POWER.md",
    "steering-templates/**/*",
    "mcp.json"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "dev": "tsc --watch"
  },
  "keywords": [
    "kiro",
    "power",
    "rag",
    "observability",
    "monitoring",
    "retrieval-augmented-generation",
    "ai",
    "machine-learning",
    "error-tracking",
    "performance-monitoring"
  ],
  "author": "RAG Observability Power Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikeartee/RAG-Observability.git"
  },
  "bugs": {
    "url": "https://github.com/mikeartee/RAG-Observability/issues"
  },
  "homepage": "https://github.com/mikeartee/RAG-Observability#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "fast-check": "^3.15.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
